int main(int ac, char **av) 
{ 
   int fd = open(av[1], O_RDONLY); 
   printf("%s\n", get_next_line(fd)); 
   printf("%s\n", get_next_line(fd)); 
   printf("%s\n", get_next_line(fd)); 
   return (0); 
} 

int main(void) 
{ 
   char *s = get_next_line(0); 

       while (s) { 
       printf("%s", s); 
       putchar('\n') ; 
       free (s) ; 
       s = get_next_line (0) ; 
   } 
   return (0); 
}  